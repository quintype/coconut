<div class="card__element--references">
  {% for reference in story_element['story-elements'] %}
    <div class="card__element--reference">
      <h3>{{ reference['metadata']['name'] }}</h3>
      <a href="{{ reference['metadata']['url'] }}">{{ reference['metadata']['url'] }}</a>
      <p>{{ reference['metadata']['description'] }}</p>
    </div>
  {% endfor %}
</div>
